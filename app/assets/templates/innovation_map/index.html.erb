<div id="wrapper">
  <section class="heading b-b">
    <div class="row no-gutter b-b">
      <div class="col-xs-2 col-sm-2 col-md-1">
        <section class="heading-btn">
          <a href="#" ng-click="backPrevLocation($event)"><i class="fas fa-long-arrow-alt-left"></i></a>
        </section>
      </div>
      <div class="col-xs-10 col-sm-10 col-md-8 b-l b-r-md">
        <section class="heading-title">
          <h1 translate>{{ 'innovation_map' }}</h1>
        </section>
      </div>
    </div>
  </section>

  <section id="map-container">
    <div id="map-content">
      <div id="map"></div>
      <div id="loading">
          <div class="loader"></div>
          <h3 class="loader-text">Finding Spaces...</h3>
      </div>
    </div>

    <div id="filter-bar"></div>

    <%= stylesheet_link_tag 'map.stylesheet' %>
    <%= javascript_include_tag 'innovation-map' %>
  </section>
</div>
